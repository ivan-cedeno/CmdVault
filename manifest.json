{
  "manifest_version": 3,
  "name": "CdmVault",
  "version": "1.0.0",
  "description": "Command Utility for Product Support Engineers.",
  "permissions": [
    "sidePanel",
    "storage",
    "tabs"
    
  ],
  
 "icons": {
  "16": "icon.png", "48": "icon.png", "128": "icon.png" },

  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Abrir Helper"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  }
  
}
